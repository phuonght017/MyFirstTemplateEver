<%- include('partials/head.ejs'); %>
<body>
    <%- include('partials/navbar.ejs'); %>
    <h1> Your random number is: <%= rand %></h1>
    <% if (rand % 2 === 0) { %>
        <h2>This is an even number!</h2>
    <% } else { %>
        <h2>This is an odd number!</h2>
    <% } %>
    <h3>This number is <%= rand % 2 === 0 ? 'EVEN' : 'ODD' %></h3>
</body>
</html>